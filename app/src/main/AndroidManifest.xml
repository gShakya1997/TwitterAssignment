<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.study.twitter">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".activities.registerActivities.RegisterActivityFourthStep"></activity>
        <activity android:name=".activities.registerActivities.RegisterActivityThirdStep" />
        <activity android:name=".activities.webviewActivities.WebViewHelp" />
        <activity android:name=".activities.webviewActivities.WebViewPP" />
        <activity android:name=".activities.webviewActivities.WebViewPO" />
        <activity android:name=".activities.webviewActivities.WebViewTerms" />
        <activity android:name=".activities.webviewActivities.WebViewCookie" />
        <activity android:name=".activities.registerActivities.RegisterActivitySecondStep" />
        <activity android:name=".activities.registerActivities.RegisterActivityPhone" />
        <activity android:name=".activities.registerActivities.RegisterActivityEmail" />
        <activity android:name=".activities.LoginActivity" />
        <activity android:name=".activities.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.GettingStartedActivity" />
    </application>

</manifest>